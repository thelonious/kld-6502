; LUIGI DEMO
; 8.26.2003
;
; Simple technology demo of the 6502
; emulator and simulated XGS GPU.
;
; NOTE:
; Just so you don't wind up surprised,
; the collision detection is unfinished
; since we just wanted fast resultS.
; Essentially you only fall through
; empty tiles, meaning even clouds will
; slam into you. :)

; **** CONSTANTS **********************


API_Hook:               .EQ $FFFF

; General API Functions
API_Exit:               .EQ $01
API_Get_Rand:           .EQ $02
API_Get_Key_State:      .EQ $03

; GPU Functions
GPU_Load_Sprites:       .EQ $0F
GPU_Set_Color:          .EQ $04
GPU_Draw_Point:         .EQ $05
GPU_Draw_Line:          .EQ $06
GPU_Draw_String:        .EQ $07
GPU_Draw_Sprite:        .EQ $08
GPU_Draw_Sprite_16x16:  .EQ $09
GPU_Draw_Screen:        .EQ $0A
GPU_Lock_Video:         .EQ $0B
GPU_Unlock_Video:       .EQ $0C
GPU_Clear_Screen:       .EQ $0D
GPU_Flip:               .EQ $0E

; Directions
UP:                     .EQ 0
DOWN:                   .EQ 1
LEFT:                   .EQ 2
RIGHT:                  .EQ 3

; Directions
KEY_UP:                 .EQ $C8
KEY_DOWN:               .EQ $D0
KEY_LEFT:               .EQ $CB
KEY_RIGHT:              .EQ $CD
KEY_SPACE:              .EQ $39

; Luigi
LUIGI_WIDTH:            .EQ 16
LUIGI_HEIGHT:           .EQ 32
LUIGI_WIDTH_HALF:       .EQ 8
LUIGI_HEIGHT_HALF:      .EQ 16
LUIGI_HEIGHT_HALF_2:    .EQ 16
LUIGI_MOVE_DIST:        .EQ 2
LUIGI_MOVE_DELAY:       .EQ 2
LUIGI_ANIM_DELAY:       .EQ 3
MAX_LUIGI_YVEL:         .EQ 64

; Game World
MAX_SCREEN:             .EQ 2
ANIM_DELAY:             .EQ 12

; **** ZERO PAGE DATA *****************

.ORG $0
key_map:    .DB $FF, $00

; Luigi animation pointer table
luigi_anim: .DB $00, $00
            .DB $00, $00
            .DB $00, $00
            .DB $00, $00
            .DB $00, $00
            .DB $00, $00
            .DB $00, $00
            .DB $00, $00

; Long Pointer
long_ptr:
long_ptr_h: .DB 0
long_ptr_l: .DB 0

; **** DATA ***************************

.ORG $600

; Global parameter passing area
api_func:   .DB 0
p_0:        .DB 0
p_1:        .DB 0
p_2:        .DB 0
p_3:        .DB 0
p_x_0:      .DB 0
p_y_0:      .DB 0
p_x_1:      .DB 0
p_y_1:      .DB 0
p_r_0:      .DB 0
p_g_0:      .DB 0
p_b_0:      .DB 0

; Global return value passing area
r_0:        .DB 0
r_1:        .DB 0
r_2:        .DB 0
r_3:        .DB 0

; Temporary registers
tr_0:       .DB 0
tr_1:       .DB 0
tr_2:       .DB 0
tr_3:       .DB 0

; Misc. flags
key_press:  .DB 0

; Luigi status
luigi_x:    .DB 192
luigi_y:    .DB 192
luigi_dir:  .DB 0
luigi_frame:.DB 0
luigi_mt:   .DB 0
luigi_at:   .DB 0
luigi_yv:   .DB 0
luigi_ga:   .DB 1
luigi_jump: .DB 0
luigi_jkey: .DB 0

; Game world status
curr_screen:.DB 0
anim_timer: .DB 0
anim_flag:  .DB 0

; Hardcoded ruit animations
fruit_0_i:  .DB 131
fruit_0_d:  .DB 16
fruit_0_mt: .DB 32
fruit_1_i:  .DB 87
fruit_1_d:  .DB 16
fruit_1_mt: .DB 64
fruit_2_i:  .DB 89
fruit_2_d:  .DB 16
fruit_2_mt: .DB 24
fruit_3_i:  .DB 51
fruit_3_d:  .DB 16
fruit_3_mt: .DB 24
fruit_4_i:  .DB 123
fruit_4_d:  .DB 16
fruit_4_mt: .DB 24
fruit_5_i:  .DB 92
fruit_5_d:  .DB 16
fruit_5_mt: .DB 24
pulley_0_i: .DB 148
pulley_0_i2:.DB 164
pulley_0_d: .DB 1
pulley_0_mt:.DB 12
pulley_1_i: .DB 152
pulley_1_i2:.DB 168
pulley_1_d: .DB 1
pulley_1_mt:.DB 12
pulley_2_i: .DB 147
pulley_2_i2:.DB 163
pulley_2_d: .DB 1
pulley_2_mt:.DB 32

; **** CODE ***************************

.ORG $A00

    ; **** Set up the luigi frame
    ; **** vector table

    LDX #0

    LDA #luigi_r0.h
    STA luigi_anim, X
    INX
    LDA #luigi_r0.l
    STA luigi_anim, X
    INX

    LDA #luigi_r1.h
    STA luigi_anim, X
    INX
    LDA #luigi_r1.l
    STA luigi_anim, X
    INX

    LDA #luigi_r2.h
    STA luigi_anim, X
    INX
    LDA #luigi_r2.l
    STA luigi_anim, X
    INX

    LDA #luigi_r1.h
    STA luigi_anim, X
    INX
    LDA #luigi_r1.l
    STA luigi_anim, X
    INX

    LDA #luigi_l0.h
    STA luigi_anim, X
    INX
    LDA #luigi_l0.l
    STA luigi_anim, X
    INX

    LDA #luigi_l1.h
    STA luigi_anim, X
    INX
    LDA #luigi_l1.l
    STA luigi_anim, X
    INX

    LDA #luigi_l2.h
    STA luigi_anim, X
    INX
    LDA #luigi_l2.l
    STA luigi_anim, X
    INX

    LDA #luigi_l1.h
    STA luigi_anim, X
    INX
    LDA #luigi_l1.l
    STA luigi_anim, X
    INX

    ; Initialize the animation timer
    LDA #ANIM_DELAY
    STA anim_timer

    ; **** Load tile bitmaps into the
    ; **** GPU sprite bank
    LDA #tile_base.h
    STA p_0
    LDA #tile_base.l
    STA p_1
    LDA #48
    STA p_2
    LDA #GPU_Load_Sprites
    STA api_func
    JSR API_Hook

main_loop:

    ; **** Draw the tile map background
    LDA curr_screen
    CLC
    ADC #screen_base.h
    STA p_0
    LDA #screen_base.l
    STA p_1
    LDA #16
    STA p_x_0
    LDA #16
    STA p_y_0
    LDA #GPU_Draw_Screen
    STA api_func
    JSR API_Hook

    ; **** Lock the video buffer
    LDA #GPU_Lock_Video
    STA api_func
    JSR API_Hook

    ; **** Determine Luigi's animation
    ; **** frame
    LDA luigi_frame
    ASL
    CLC
    ADC luigi_dir
    TAX

    ; **** Draw Luigi
    LDA luigi_x
    SEC
    SBC #LUIGI_WIDTH_HALF
    STA p_x_0
    LDA luigi_y
    SEC
    SBC #LUIGI_HEIGHT_HALF
    STA p_y_0
    LDA luigi_anim, X
    STA p_0
    INX
    LDA luigi_anim, X
    STA p_1
    LDA #GPU_Draw_Sprite
    STA api_func
    JSR API_Hook

    ; **** Unlock the video buffer
    LDA #GPU_Unlock_Video
    STA api_func
    JSR API_Hook

    ; **** Blit the frame buffer
    ; **** to the screen
    LDA #GPU_Flip
    STA api_func
    JSR API_Hook

    ; **** Clear the frame buffer to
    ; **** the blue sky color
    LDA #%00101011
    STA p_0
    LDA #%10111111
    STA p_1
    LDA #GPU_Clear_Screen
    STA api_func
    JSR API_Hook

    ; **** Grab the keyboard status
    LDA #API_Get_Key_State
    STA api_func
    JSR API_Hook

    LDA #0
    STA key_press

    ; **** Check for the exit key
    ; **** (escape)
    LDY #$01
    LDA ( key_map ), Y
    BEQ skip_exit
    LDA #API_Exit
    STA api_func
    JSR API_Hook

skip_exit:

    ; **** Check for movement
    LDA luigi_mt
    BEQ do_luigi_nav
    JMP skip_luigi_nav

do_luigi_nav:

    ; **** Space (Jump)
    LDY #KEY_SPACE
    LDA ( key_map ), Y
    BEQ skip_jump_key
    LDA luigi_jump
    BEQ skip_jump
    LDA luigi_jkey
    BNE skip_jump
    LDA #-8
    STA luigi_yv
    LDA #1
    STA luigi_jkey

skip_jump:
    LDA #1
    STA key_press
    JMP no_reset_jkey

skip_jump_key:

    ; **** Also unset the jump key
    ; **** flag, to allow another jump
    LDA #0
    STA luigi_jkey

no_reset_jkey:

    ; **** Left
    LDY #KEY_LEFT
    LDA ( key_map ), Y
    BEQ skip_move_left
    LDA luigi_x
    SEC
    SBC #LUIGI_MOVE_DIST
    STA luigi_x
    LDA #8
    STA luigi_dir

    LDA luigi_at
    BNE skip_anim_right
    LDA luigi_frame
    CLC
    ADC #1
    CMP #4
    BCC skip_clip_anim_right
    LDA #0

skip_clip_anim_right:
    STA luigi_frame

skip_anim_right:

    LDA luigi_x
    CMP #8
    BCS skip_prev_screen
    LDA curr_screen
    BNE not_first_screen
    LDA #MAX_SCREEN
    STA curr_screen
    JMP prev_screen_done

not_first_screen:
    DEC curr_screen

prev_screen_done:
    LDA #248
    STA luigi_x

skip_prev_screen:
    LDA #1
    STA key_press

skip_move_left:

    ; **** Right
    LDY #KEY_RIGHT
    LDA ( key_map ), Y
    BEQ skip_move_right
    LDA luigi_x
    CLC
    ADC #LUIGI_MOVE_DIST
    STA luigi_x
    LDA #0
    STA luigi_dir

    LDA luigi_at
    BNE skip_anim_left
    LDA luigi_frame
    CLC
    ADC #1
    CMP #4
    BCC skip_clip_anim_left
    LDA #0

skip_clip_anim_left:
    STA luigi_frame

skip_anim_left:

    LDA luigi_x
    CMP #248
    BCC skip_next_screen
    LDA curr_screen
    CMP #MAX_SCREEN
    BNE not_last_screen
    LDA #0
    STA curr_screen
    JMP next_screen_done

not_last_screen:
    INC curr_screen

next_screen_done:
    LDA #8
    STA luigi_x

skip_next_screen:
    LDA #1
    STA key_press

skip_move_right:

    ; Skip the following block if any
    ; key was pressed
    LDA key_press
    BNE key_pressed

    ; No key was pressed, so reset the
    ; movement animation
    LDA #0
    STA luigi_frame

skip_luigi_nav:

key_pressed:

    ; **** Update Luigi's gravity
    LDA luigi_mt
    BEQ do_luigi_grav
    JMP skip_luigi_grav

do_luigi_grav:

    LDA luigi_y
    CLC
    ADC luigi_yv
    STA luigi_y
    LDA luigi_yv
    CLC
    ADC luigi_ga
    STA luigi_yv
    CMP #MAX_LUIGI_YVEL
    ;BCC skip_clip_yv
    BMI skip_clip_yv
    LDA #MAX_LUIGI_YVEL
    STA luigi_yv

skip_clip_yv:

    ; **** Handle vertical bounds
    ; **** checking
    LDA luigi_y
    CMP #16
    BCS skip_clip_top
    LDA #16
    STA luigi_y
    LDA #0
    STA luigi_yv

skip_clip_top:

    ; **** Handle collision detection/
    ; **** normal force
    LDA #0
    STA luigi_jump

    ; Only check downward collisions
    ; if Luigi is moving down
    LDA luigi_yv
    BPL do_luigi_down_col
    JMP skip_luigi_down_col

do_luigi_down_col:

    ; Determine Luigi's tile
    ; coordinates
    LDA luigi_x
    LSR
    LSR
    LSR
    LSR
    STA tr_0
    LDA luigi_y
    CLC
    ADC #LUIGI_HEIGHT_HALF_2
    ;ADC #1
    LSR
    LSR
    LSR
    LSR
    STA tr_1
    ASL
    ASL
    ASL
    ASL
    CLC
    ADC tr_0
    TAY
    LDA #screen_base.h
    CLC
    ADC curr_screen
    STA long_ptr_h
    LDA #screen_base.l
    STA long_ptr_l
    LDA ( long_ptr ), Y
    SEC
    SBC #46
    BEQ skip_collision

    ; Luigi has hit the ground, so
    ; stabalize his Y position above
    ; the tile
    LDA tr_1
    SEC
    SBC #1
    ASL
    ASL
    ASL
    ASL
    ;SEC
    ;SBC #14
    STA luigi_y
    ;LDA luigi_yv
    ;SBC luigi_ga
    LDA #0
    STA luigi_yv
    LDA #1
    STA luigi_jump

skip_collision:

skip_luigi_grav:

skip_luigi_down_col:

    ; **** Update timers
    LDA luigi_mt
    BNE skip_reset_luigi_mt
    LDA #LUIGI_MOVE_DELAY
    STA luigi_mt

skip_reset_luigi_mt:

    DEC luigi_mt
    LDA luigi_at
    BNE skip_reset_luigi_at
    LDA #LUIGI_ANIM_DELAY
    STA luigi_at

skip_reset_luigi_at:

    DEC luigi_at

; *************************************
; ** BEGIN HARDCODED TILE ANIMATIONS **
; *************************************

    DEC fruit_0_mt
    BNE skip_anim_fruit_0
    LDA #32
    STA fruit_0_mt
    LDX fruit_0_i
    LDA #'?'
    STA screen_0, X
    LDA fruit_0_i
    CLC
    ADC fruit_0_d
    STA fruit_0_i
    LDX fruit_0_i
    LDA #'J'
    STA screen_0, X
    LDA fruit_0_i
    CMP #68
    BCC flip_fruit_0
    CMP #147
    BCS flip_fruit_0
    JMP skip_anim_fruit_0
flip_fruit_0:
    LDA fruit_0_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_0_d
skip_anim_fruit_0:
    DEC fruit_1_mt
    BNE skip_anim_fruit_1
    LDA #32
    STA fruit_1_mt
    LDX fruit_1_i
    LDA #'?'
    STA screen_0, X
    LDA fruit_1_i
    CLC
    ADC fruit_1_d
    STA fruit_1_i
    LDX fruit_1_i
    LDA #'K'
    STA screen_0, X
    LDA fruit_1_i
    CMP #87
    BCC flip_fruit_1
    CMP #88
    BCS flip_fruit_1
    JMP skip_anim_fruit_1
flip_fruit_1:
    LDA fruit_1_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_1_d
skip_anim_fruit_1:
    DEC fruit_2_mt
    BNE skip_anim_fruit_2
    LDA #32
    STA fruit_2_mt
    LDX fruit_2_i
    LDA #'?'
    STA screen_0, X
    LDA fruit_2_i
    CLC
    ADC fruit_2_d
    STA fruit_2_i
    LDX fruit_2_i
    LDA #'L'
    STA screen_0, X
    LDA fruit_2_i
    CMP #89
    BCC flip_fruit_2
    CMP #136
    BCS flip_fruit_2
    JMP skip_anim_fruit_1
flip_fruit_2:
    LDA fruit_2_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_2_d
skip_anim_fruit_2:
    DEC fruit_3_mt
    BNE skip_anim_fruit_3
    LDA #32
    STA fruit_3_mt
    LDX fruit_3_i
    LDA #'?'
    STA screen_1, X
    LDA fruit_3_i
    CLC
    ADC fruit_3_d
    STA fruit_3_i
    LDX fruit_3_i
    LDA #'K'
    STA screen_1, X
    LDA fruit_3_i
    CMP #51
    BCC flip_fruit_3
    CMP #83
    BCS flip_fruit_3
    JMP skip_anim_fruit_1
flip_fruit_3:
    LDA fruit_3_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_3_d
skip_anim_fruit_3:
    DEC fruit_4_mt
    BNE skip_anim_fruit_4
    LDA #32
    STA fruit_4_mt
    LDX fruit_4_i
    LDA #'?'
    STA screen_1, X
    LDA fruit_4_i
    CLC
    ADC fruit_4_d
    STA fruit_4_i
    LDX fruit_4_i
    LDA #'J'
    STA screen_1, X
    LDA fruit_4_i
    CMP #123
    BCC flip_fruit_4
    CMP #123
    BCS flip_fruit_4
    JMP skip_anim_fruit_1
flip_fruit_4:
    LDA fruit_4_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_4_d
skip_anim_fruit_4:
    DEC fruit_5_mt
    BNE skip_anim_fruit_5
    LDA #32
    STA fruit_5_mt
    LDX fruit_5_i
    LDA #'?'
    STA screen_2, X
    LDA fruit_5_i
    CLC
    ADC fruit_5_d
    STA fruit_5_i
    LDX fruit_5_i
    LDA #'L'
    STA screen_2, X
    LDA fruit_5_i
    CMP #92
    BCC flip_fruit_5
    CMP #108
    BCS flip_fruit_5
    JMP skip_anim_fruit_5
flip_fruit_5:
    LDA fruit_5_d
    EOR #$FF
    CLC
    ADC #1
    STA fruit_5_d
skip_anim_fruit_5:
    DEC pulley_0_mt
    BNE skip_anim_pulley_0
    LDA #32
    STA pulley_0_mt
    LDX pulley_0_i2
    LDA #'.'
    STA screen_0, X
    LDX pulley_0_i
    LDA #'9'
    STA screen_0, X
    LDA pulley_0_i2
    CLC
    ADC pulley_0_d
    STA pulley_0_i2
    LDA pulley_0_i
    CLC
    ADC pulley_0_d
    STA pulley_0_i
    LDX pulley_0_i
    LDA #'Y'
    STA screen_0, X
    LDX pulley_0_i2
    LDA #'Z'
    STA screen_0, X
    LDA pulley_0_i2
    CMP #165
    BCC flip_pulley_0
    CMP #172
    BCS flip_pulley_0
    JMP skip_anim_pulley_0
flip_pulley_0:
    LDA pulley_0_d
    EOR #$FF
    CLC
    ADC #1
    STA pulley_0_d
skip_anim_pulley_0:
    DEC pulley_1_mt
    BNE skip_anim_pulley_1
    LDA #32
    STA pulley_1_mt
    LDX pulley_1_i2
    LDA #'.'
    STA screen_0, X
    LDX pulley_1_i
    LDA #'9'
    STA screen_0, X
    LDA pulley_1_i2
    CLC
    ADC pulley_1_d
    STA pulley_1_i2
    LDA pulley_1_i
    CLC
    ADC pulley_1_d
    STA pulley_1_i
    LDX pulley_1_i
    LDA #'Y'
    STA screen_0, X
    LDX pulley_1_i2
    LDA #'Z'
    STA screen_0, X
    LDA pulley_1_i2
    CMP #165
    BCC flip_pulley_1
    CMP #172
    BCS flip_pulley_1
    JMP skip_anim_pulley_1
flip_pulley_1:
    LDA pulley_1_d
    EOR #$FF
    CLC
    ADC #1
    STA pulley_1_d
skip_anim_pulley_1:
    DEC pulley_2_mt
    BNE skip_anim_pulley_2
    LDA #32
    STA pulley_2_mt
    LDX pulley_2_i2
    LDA #'.'
    STA screen_1, X
    LDX pulley_2_i
    LDA #'9'
    STA screen_1, X
    LDA pulley_2_i2
    CLC
    ADC pulley_2_d
    STA pulley_2_i2
    LDA pulley_2_i
    CLC
    ADC pulley_2_d
    STA pulley_2_i
    LDX pulley_2_i
    LDA #'Y'
    STA screen_1, X
    LDX pulley_2_i2
    LDA #'Z'
    STA screen_1, X
    LDA pulley_2_i2
    CMP #162
    BCC flip_pulley_2
    CMP #165
    BCS flip_pulley_2
    JMP skip_anim_pulley_2
flip_pulley_2:
    LDA pulley_2_d
    EOR #$FF
    CLC
    ADC #1
    STA pulley_2_d
skip_anim_pulley_2:
    DEC anim_timer
    BEQ no_skip_anim
    JMP skip_anim
no_skip_anim:
    LDA #ANIM_DELAY
    STA anim_timer
    LDA #API_Get_Rand
    STA api_func
    JSR API_Hook
    LDA anim_flag
    BEQ skip_anim_0
    LDA #0
    STA anim_flag
    LDX #213
    LDA #'O'
    STA screen_0, X
    LDX #43
    LDA #'O'
    STA screen_1, X
    LDX #185
    LDA #'O'
    STA screen_2, X
    LDA r_0
    CMP #127
    BCS no_anim_dk_0
    LDX #16
    LDA #'A'
    STA screen_0, X
    LDX #17
    LDA #'B'
    STA screen_0, X
    LDX #18
    LDA #'C'
    STA screen_0, X
    LDX #19
    LDA #'D'
    STA screen_0, X
    LDX #32
    LDA #'E'
    STA screen_0, X
    LDX #33
    LDA #'F'
    STA screen_0, X
    LDX #34
    LDA #'G'
    STA screen_0, X
    LDX #35
    LDA #'H'
    STA screen_0, X
no_anim_dk_0:
    JMP skip_anim
skip_anim_0:
    LDA #1
    STA anim_flag
    LDA r_0
    CMP #127
    BCS no_anim_dk_1
    LDX #16
    LDA #'Q'
    STA screen_0, X
    LDX #17
    LDA #'R'
    STA screen_0, X
    LDX #18
    LDA #'S'
    STA screen_0, X
    LDX #19
    LDA #'T'
    STA screen_0, X
    LDX #32
    LDA #'U'
    STA screen_0, X
    LDX #33
    LDA #'V'
    STA screen_0, X
    LDX #34
    LDA #'W'
    STA screen_0, X
    LDX #35
    LDA #'X'
    STA screen_0, X
no_anim_dk_1:
    LDX #213
    LDA #'P'
    STA screen_0, X
    LDX #43
    LDA #'P'
    STA screen_1, X
    LDX #185
    LDA #'P'
    STA screen_2, X
skip_anim:
skip_reset_left:

    JMP main_loop

    ; **** Terminate the program
    LDA #API_Exit
    STA api_func
    JSR API_Hook

; **** ASSETS *************************

.ORG $8000

; **** SCREEN MAPS ********************

screen_base:

screen_0:  .DB "7.....?..?67...."
           .DB "ABCD//?......@/@"
           .DB "EFGHI........?.?"
           .DB "/@/@/@/@/@.@/?.?"
           .DB ".?.?.?.?.?.?.?.?"
           .DB ".?.?.?.?.?.?.?.?"
           .DB "...?.?.?.?...?.?"
           .DB ".:2?.?...?......"
           .DB ".<5?.....?.67..."
           .DB "7<5?99999999998."
